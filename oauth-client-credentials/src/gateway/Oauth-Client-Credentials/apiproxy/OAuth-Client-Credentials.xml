<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy revision="1" name="OAuth-Client-Credentials">
    <Basepaths>/v1/cc/oauth_cc</Basepaths>
    <ConfigurationVersion majorVersion="4" minorVersion="0"/>
    <CreatedAt>1478158501622</CreatedAt>
    <CreatedBy>nisha@digitalapicraft.com</CreatedBy>
    <Description>A sampleAPI proxy that enforces OAuth 2.0 client credentials</Description>
    <DisplayName>Certification_OAuthClientCredentials</DisplayName>
    <LastModifiedAt>1478159537414</LastModifiedAt>
    <LastModifiedBy>nisha@digitalapicraft.com</LastModifiedBy>
    <Policies>
        <Policy>GenerateAccessToken</Policy>
        <Policy>OAuth-Verify-Token</Policy>
        <Policy>return-valid-response</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources>
        <Resource>jsc://return-valid-response.js</Resource>
    </Resources>
    <Spec></Spec>
    <TargetServers/>
    <TargetEndpoints>
        <TargetEndpoint>default</TargetEndpoint>
    </TargetEndpoints>
    <validate>false</validate>
</APIProxy>
